    android:defaultValue="0"
    android:key="@string/other_autotimestop_default_key"
    android:summary="Please enter your time\n(0 sec is disable)."
    android:title="@string/settings_autotimestop_default_title" />  
  <org.ozonecity.android.apps.mytracks.settings.MyTracksEditTextPreference
    android:key="@string/other_link_default_key"
