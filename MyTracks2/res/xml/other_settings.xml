<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
  android:title="@string/settings_other" >
  <org.ozonecity.android.apps.mytracks.settings.MyTracksEditTextPreference
    android:key="@string/other_language_default_key"
    android:summary="Please enter your languase code\n(Test preference only, no effect)."
    android:title="@string/settings_language_default_title" />
  <org.ozonecity.android.apps.mytracks.settings.MyTracksEditTextPreference
